$nullString := function($x){
  ($x in $nullableValues or $type($x) != "string" or $not($exists($x)) ) ? null : $x
};

$nullList := function($x){
    [$filter($x, function($xx){
        $map($nullString($xx), function($item){ $item != null })
    })]
};


