];

$getMaterialType := function($tissue){
    ($type($tissue) = "string" ? $tissue : $tissue.materialType) ~> $cleanEnumSingle($allowedMaterialTypes) ~> $defaultValue("None/not available")
};

$isValidString := function($str) { $type($str) = "string" and $str != null and $str != "" };
$isValidArray := function($arr) { $type($arr) = "array"  };

$tryMatchEnum := function($x, $allowed){(
    $cleaned := $x ? $x : ""; 
    $lowered := $join([$uppercase($substring($cleaned, 0, 1)), $substring($lowercase($cleaned), 1, $length($cleaned))]);
    $match := $filter($allowed, function($v, $i, $a){(
        $allowedLowered := $join([$uppercase($substring($v, 0, 1)), $substring($lowercase($v), 1, $length($v))]);
        $allowedLowered = $lowered ? $a[$i] : null
    )})
)};


$cleanEnum := function($x, $allowed){
    ($isValidString($x) ? 
        [$map($split($x, ","), function($v){
            $tryMatchEnum($v, $allowed)
        })] : 
        ($isValidArray($x) ? [$map($x, function($v){
            $tryMatchEnum($v, $allowed)
        })] : null )
    )
};

$cleanEnumSingle := function($x, $allowed){
    $tryMatchEnum($x, $allowed)
};




{
    "identifier": input.identifier,
    "issued": input.issued,
    "modified": input.modified,
    "revisions": input.revisions,
    "version": input.version,
    "summary": {
        "abstract": input.summary.abstract,
        "contactPoint": input.summary.contactPoint,
        "keywords": input.summary.keywords,
        "doiName": input.summary.doiName,
        "title": input.summary.title,
        "dataCustodian": {
            "name": input.summary.dataCustodian.name,
            "identifier": input.summary.dataCustodian.identifier,
            "contactPoint": input.summary.dataCustodian.contactPoint,
            "logo": input.summary.dataCustodian.logo
        },
        "populationSize": input.summary.populationSize,
        "funders": input.summary.funders,
        "datasetAliases": input.summary.datasetAliases
    },
    "documentation": input.documentation ? {
        "description": input.documentation.description,
        "associatedMedia": input.documentation.associatedMedia,
        "inPipeline": input.documentation.inPipeline
    } : null,
    "coverage":  input.coverage ~> |$|{
        "typicalAgeRangeMin": $ageRangeDefault(typicalAgeRangeMin, null),
        "typicalAgeRangeMax": $ageRangeDefault(typicalAgeRangeMax, null),
        "materialType": materialType ? [$map(materialType, $getMaterialType)] : ["None/not available"]
    },['datasetCompleteness','pathway','followUp']|, 
    "provenance": {
        "origin": input.provenance.origin ? {
            "purpose": input.provenance.origin.purpose  ~> $safeSplit(';,;') ~> $cleanEnum($allowedPurpose),
            "source": input.provenance.origin.source  ~> $safeSplit(';,;') ~> $cleanEnum($allowedSource),
            "collectionSource": input.provenance.origin.collectionSource  ~> $safeSplit(';,;') ~> $cleanEnum($allowedSetting),
            "datasetType": [
                $map(input.provenance.origin.datasetType, function($dt) {(
                $mainKey := $dt.name;
                $sub := $type($dt.subTypes) = "array" ? $dt.subTypes : [];
                $lookUpList :=[$lookup($datasetMap, $mainKey)];
                $allowedSubs := $filter(
                    $map($sub, function($sx) {
                      (
                    $mapped := $sx = "Labour" ? "Occupation" : $sx = "Finances" ? "Economics" : $sx;
                    $mapped in $lookUpList ? $mapped : null
                    )
                    }),
                    function($v) { $v }
                );
                {
                    "name": $mainKey,
                    "subTypes": [$allowedSubs]
                }
               
            )
            })],
            "imageContrast": input.provenance.origin.imageContrast
        } : null,
        "temporal": { /* input.provenance.temporal */
            "endDate": input.provenance.temporal.endDate ~> $nullString,
            "startDate": input.provenance.temporal.startDate ~> $nullString ~> $defaultValue("1970-01-01"),
            "timeLag": $cleanEnumSingle(input.provenance.temporal.timeLag, $allowedTimeLag) ~> $defaultValue('Not applicable'),
            "publishingFrequency": $cleanEnumSingle(input.provenance.temporal.publishingFrequency, $allowedPublishingFrequency) ~> $defaultValue("Other"),
            "distributionReleaseDate": input.provenance.temporal.distributionReleaseDate
        }
    },
    "accessibility": input.accessibility ~> |$|{
        "access": access ~> |$|{
            "accessServiceCategory": accessServiceCategory ~> $cleanEnumSingle($allowedServiceCategory) ~> $defaultValue(null),
            "jurisdiction": $safeSplit(jurisdiction, ";,;")
        }|,  
        "usage": usage ? usage ~> |$|{
            "dataUseRequirements": $safeSplit(dataUseRequirements,";,;") ~> $cleanEnum($allowedDataUseRequirements) ~> $defaultValue(null),
            "dataUseLimitation": $safeSplit(dataUseLimitation,";,;") ~> $cleanEnum($allowedDataUseLimitation) ~> $defaultValue(null),
            "resourceCreator": resourceCreator
        }|,
        "formatAndStandards": formatAndStandards ? formatAndStandards ~> |$|{
            "vocabularyEncodingScheme": vocabularyEncodingScheme ?
                $safeSplit(vocabularyEncodingScheme, ";,;") ~> $cleanEnum($allowedEncodings)  : [],
            "language": $safeSplit(language, ";,;"),
            "format": $safeSplit(format, ";,;"),
            "conformsTo": $safeSplit(conformsTo, ";,;") ~> $cleanEnum($allowedStandards) ~> $defaultValue([])
        }, ['vocabularyEncodingScheme','language','format']| : null
    }|,
    "enrichmentAndLinkage": input.enrichmentAndLinkage ? input.enrichmentAndLinkage ~> |$|{
        "tools": tools,
        "investigations": investigations,
        "derivedFrom": derivedFrom,
        "isPartOf": isPartOf,
        "linkableDatasets": linkableDatasets,
        "similarToDatasets": similarToDatasets
    }| : null,
    "datasetFilters": input.datasetFilters,
    "project": input.project,
    "icons": input.icons,
    "erd": input.erd,
    "observations": input.observations ~> $defaultValue([]),
    "structuralMetadata": input.structuralMetadata ? {
        "tables": input.structuralMetadata.tables,
        "syntheticDataWebLink": input.structuralMetadata.syntheticDataWebLink
